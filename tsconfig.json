{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["es2017"],
    "module": "commonjs",
    "allowJs": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "baseUrl": "."
  },
  "exclude": ["node_modules"]
}
